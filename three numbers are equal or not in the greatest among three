#include <stdio.h>

int main()
{
    int a,b,c;
    printf("Enter first number:");
    scanf("%d",&a);
    printf("Enter second number:");
    scanf("%d",&b);
    printf("Enter third number:");
    scanf("%d",&c);
    
    if (a>b && a>c)
    {
        printf("%d is Greater ",a);
    }
    else if(b>a && b>c)
{
    printf("%d is Greater",b);
}
   else{
       printf("%d is Greater",c);
   }
   if (a==b && b==c){
       printf(" \nThe three numbers are equal");
   }
   else {
       printf("\nThe three numbers are not Equal");
      
   }
    
    return 0;
}
