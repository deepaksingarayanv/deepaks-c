include <stdio.h>

void swap(int *a, int *b) {
    int temp = *a;
    *a = *b;
    *b = temp;
}

int main() {
    int x = printf("enter an integer: ");
            scanf("%d",&x);
            printf("you entered= %d\n, x,y");
    //int B
    int y = printf("enter an integer: ");
            scanf("%d",&y);
            printf("you entered= %d\n, x,y");
    
    printf("Before swap: x = %d, y = %d\n", x, y);
    swap(&x, &y);
    printf("After swap: x = %d, y = %d\n", x, y);
    return 0;
}
