#include<stdio.h>

int main(){
    int a,b;
    printf("Enter first number:");
    scanf("%d",&a);
    printf("Enter second number:");
    scanf("%d",&b);
    
    if(a > b)
    {
    printf("%d  is maximum",a);
    }
    else
    {
        printf("%d",b);
    }
    if(a < b)
    {
    printf(" is maximum");
    }
    


    return 0;
}
